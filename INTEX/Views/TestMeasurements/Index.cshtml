        <th>
            @Html.DisplayNameFor(model => model.CONCENTRATION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ASSAYID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
